#this function converts a grade into a concept (A, B or C)
def converter(grade):
    if grade > 90:
        return("A")
    elif grade > 80:
        return("B")
    elif grade > 70:
        return ("C")
    elif grade > 60:
        return ("D")
    elif grade > 50:
        return ("F")
grade = (int(input("Enter your grade and i will return your concept (A, B, C, D or F): ")))
print(converter(grade)) 
